<h1>All Tags</h1>

<ul id="articles">
  <% @tags.each do |tag| %>
    <li>
      <%= link_to tag.name, tag_path(tag), class: "tag.article" %>
    </li>
    <% end %>
</ul>

<%= link_to "Create a New Article", new_article_path, class: "new_article" %>
<br>
<%= link_to "<< Back to Articles List", articles_path %>
